<Window x:Class="SportFactoryApp.Members.AddMemberWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        Title="Add Member" Height="300" Width="400">
    <StackPanel Margin="20">
        <xctk:WatermarkTextBox Name="FirstNameTextBox" Watermark="Enter First Name" />
        <xctk:WatermarkTextBox Name="LastNameTextBox" Watermark="Enter Last Name" />
        <Button Name="AddMemberButton" Content="Add Member" Click="AddButton_Click"/>
    </StackPanel>
</Window>
